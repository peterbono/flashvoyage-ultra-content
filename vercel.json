{
  "version": 2,
  "builds": [
    {
      "src": "api/auto-publish.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auto-publish",
      "dest": "/api/auto-publish.js"
    }
  ],
  "crons": [
    {
      "path": "/api/auto-publish",
      "schedule": "0 9 * * *"
    }
  ]
}
